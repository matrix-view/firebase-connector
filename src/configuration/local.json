{
  "kafka-broker": "localhost:9092",
  "port": 8081,
  "kafka": {
    "clientOptions": {
      "groupId": "kafka-node-group",
      "autoCommit": true,
      "autoCommitIntervalMs": 5000,
      "fetchMaxWaitMs": 100,
      "fetchMinBytes": 1,
      "fetchMaxBytes": 1048576,
      "fromOffset": false,
      "encoding": "utf8",
      "keyEncoding": "utf8"
    },
    "consumerOptions": {
      "connectTimeout": 10000,
      "requestTimeout": 10000,
      "autoConnect": true,
      "clientId": ""
    }
  },
  "collections": [
    {
      "collection": "data",
      "topic": "data"
    }
  ]
}